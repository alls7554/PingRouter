{
  "name": "pingrouter",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "PingRouter": "./bin/www"
  },
  "scripts": {
    "start": "export NODE_ENV=development && nodemon ./bin/www",
    "prod": "export NODE_ENV=production && nodemon ./bin/www",
    "build": "pkg .",
    "build -d": "pkg -d .",
    "elec": "electron ."
  },
  "pkg": {
    "scripts": "routes/**/*.js",
    "assets": [
      "views/*",
      "public/**/*"
    ],
    "targets": [
      "node14-macos-x64", "node14-win-x86"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "mime": "^1.4.1",
    "moment": "^2.29.1",
    "mongoose": "^5.12.6",
    "mongoose-sequence": "^5.3.1",
    "morgan": "~1.9.1",
    "net-ping": "^1.2.3",
    "nodejs-traceroute": "^1.2.0",
    "npm": "^7.17.0",
    "socket.io": "^4.1.2",
    "uuid": "^3.4.0"
  },
  "description": "### You must `npm install` after cloning.",
  "main": "electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alls7554/PingRouter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alls7554/PingRouter/issues"
  },
  "homepage": "https://github.com/alls7554/PingRouter#readme",
  "devDependencies": {
    "electron": "^13.1.2",
    "http-errors": "^1.6.3"
  }
}
